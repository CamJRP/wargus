;;
;; menu-game
;;
(define-menu 'name 'menu-game 'geometry '(272 96 256 288)
    'image 'panel1 'default '7)
(define-menu-item 'pos (list 128 11) 'font 'large  'init 'init-game-menu
    'text '("Game Menu" center)
    'menu 'menu-game)
(define-menu-item 'pos (list 16 40) 'font 'large 'flags '(selected)
    'button '(size (106 27)
            caption "Save (~<F11~>)"
            hotkey "f11"
            func game-menu-save
            style gm-half)
    'menu 'menu-game)
(if (symbol-bound? 'freecraft-feature-debug ())
(define-menu-item 'pos (list (+ 16 12 106) 40) 'font 'large 'flags '(selected)
    'button '(size (106 27)
            caption "Load (~<F12~>)"
            hotkey "f12"
            func game-menu-load
            style gm-half)
    'menu 'menu-game)
(define-menu-item 'pos (list (+ 16 12 106) 40) 'font 'large 'flags '(selected)
    'flags '(disabled)
    'button '(size (106 27)
            caption "Load (~<F12~>)"
            hotkey "f12"
            func game-menu-load
            style gm-half)
    'menu 'menu-game)
)
(define-menu-item 'pos (list 16 (+ 40 (* 36 1))) 'font 'large 'flags '(selected)
    'button '(size (224 27)
